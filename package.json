  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:advanced": "vite --open ./index.advanced.html",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "start:basic": "vite serve --open ./index.basic.html",
